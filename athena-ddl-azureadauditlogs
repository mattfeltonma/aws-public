CREATE EXTERNAL TABLE `tbl_auditlogs`(
  `value` array<struct<activitydatetime:timestamp,activitydisplayname:string,additionaldetails:array<struct<key:string,value:string>>,category:string,correlationid:string,id:string,initiatedby:struct<app:struct<appid:string,displayname:string,serviceprincipalid:string,serviceprincipalname:string>,user:struct<displayname:string,id:string,ipaddress:string,userprincipalname:string>>,loggedbyservice:string,result:string,resultreason:string,targetresources:array<struct<displayname:string,id:string,modifiedproperties:array<struct<displayname:string,newvalue:string,oldvalue:string>>>>>>)
